{
  "id": "overlijdens",
  "type": "table",
  "version": "3.0.0",
  "auth": "BRP/R",
  "reasonsNonPublic": [
    "5.1 1d: Bevat persoonsgegevens"
  ],
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "identifier": "identificatie",
    "required": [
      "schema",
      "identificatie"
    ],
    "display": "identificatie",
    "properties": {
      "schema": {
        "$ref": "https://schemas.data.amsterdam.nl/schema@v1.3.0#/definitions/schema"
      },
      "identificatie": {
        "type": "string",
        "description": "identificatie, het Burgerservicenummer van de persoon op de persoonslijst (PL)"
      },
      "aNummer": {
        "type": "string",
        "description": "aNummer van de persoon op de persoonslijst (PL)"
      },
      "datumOverlijden": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date",
            "description": "datum van overlijden"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          }
        }
      },
      "plaatsOverlijden": {
        "type": "string",
        "relation": "brk2:kadastralegemeentecodes",
        "description": "code, opgenomen in Tabel 33, Gemeentetabel"
      },
      "landOverlijden": {
        "type": "string",
        "description": "code, opgenomen in Tabel 34, Landentabel"
      },
      "beginGeldigheid": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date",
            "description": "De datum waarop het geheel van gegevens geldig is geworden"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          }
        }
      },
      "eindGeldigheid": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date",
            "description": "De datum waarop de geldigheid van het geheel van gegevens beeindigd is"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          }
        }
      },
      "datumOpneming": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date",
            "description": "De datum waarop het geheel van gegevens daadwerkelijk in de GBA is opgenomen"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          }
        }
      },
      "heeftDocumenten": {
        "type": "object",
        "properties": {
          "identificatie": {
            "type": "string"
          }
        },
        "relation": "brp:documenten",
        "description": "gemeenteDocument of Akte van de categorie 01 Persoon"
      },
      "heeftOnderzoeken": {
        "type": "object",
        "properties": {
          "identificatie": {
            "type": "string"
          }
        },
        "relation": "brp:onderzoeken",
        "description": "onderzoeken  van de categorie 01 Persoon"
      }
    }
  }
}

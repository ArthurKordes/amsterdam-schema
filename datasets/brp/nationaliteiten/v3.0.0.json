{
  "id": "nationaliteiten",
  "type": "table",
  "version": "3.0.0",
  "auth": "BRP/R",
  "reasonsNonPublic": [
    "5.1 1d: Bevat persoonsgegevens"
  ],
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "identifier": "identificatie",
    "required": [
      "schema",
      "identificatie"
    ],
    "display": "identificatie",
    "properties": {
      "schema": {
        "$ref": "https://schemas.data.amsterdam.nl/schema@v1.3.0#/definitions/schema"
      },
      "identificatie": {
        "type": "string",
        "description": "identificatie, de samenstelling van het Anummer van de persoon en een volgnummer. Bijvoorbeeld 1234567890.1 (Anummer.(sequence number))."
      },
      "aNummer": {
        "type": "string",
        "description": "aNummer van de persoon."
      },
      "nationaliteit": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "code van de nationaliteit."
          },
          "omschrijving": {
            "type": "string",
            "description": "omschrijving van de nationaliteit."
          }
        },
        "description": "Een code, opgenomen in Tabel 32, Nationaliteitentabel op naam, die aangeeft welke nationaliteit de ingeschrevene bezit."
      },
      "redenVerkrijgingNedNat": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "code van de reden verkrijging"
          },
          "omschrijving": {
            "type": "string",
            "description": "omschrijving van de reden verkrijging"
          }
        },
        "description": "gegevens over de verkrijging van de Nederlandse Nationaliteit, code, opgenomen in Tabel 37"
      },
      "redenVerliesNedNat": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "code van de reden verlies"
          },
          "omschrijving": {
            "type": "string",
            "description": "omschrijving van de reden verlies"
          }
        },
        "description": "gegevens over het verlies van de Nederlandse Nationaliteit, code, opgenomen in Tabel 37"
      },
      "aanduidingBijzonderNedschap": {
        "type": "string",
        "description": "Gegevens over bijzonder Nederlanderschap"
      },
      "beginGeldigheid": {
        "type": "string",
        "format": "date",
        "description": "Complete datum: de datum waarop het geheel van gegevens geldig is geworden."
      },
      "eindGeldigheid": {
        "type": "string",
        "format": "date",
        "description": "Complete datum: de datum waarop het geheel van gegevens ongeldig is geworden."
      },
      "ingangsdatumGeldigheid": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          },
          "description": "Incomplete datum: De datum waarop het geheel van gegevens geldig is geworden."
        }
      },
      "datumOpneming": {
        "type": "object",
        "properties": {
          "datum": {
            "type": "string",
            "format": "date",
            "description": "De datum waarop het geheel van gegevens daadwerkelijk in de GBA is opgenomen"
          },
          "jaar": {
            "type": "integer",
            "description": "jaar van de datum, als deze bekend is."
          },
          "maand": {
            "type": "integer",
            "description": "maand van de datum, als deze bekend is."
          },
          "dag": {
            "type": "integer",
            "description": "dag van de datum, als deze bekend is."
          }
        }
      },
      "heeftDocument": {
        "type": "object",
        "properties": {
          "identificatie": {
            "type": "string"
          }
        },
        "relation": "brp:documenten",
        "description": "gemeenteDocument of Akte van de categorie 04 nationaliteit"
      },
      "heeftOnderzoek": {
        "type": "object",
        "properties": {
          "identificatie": {
            "type": "string"
          }
        },
        "relation": "brp:onderzoeken",
        "description": "onderzoeken  van de categorie 04 nationaliteit"
      }
    }
  }
}
